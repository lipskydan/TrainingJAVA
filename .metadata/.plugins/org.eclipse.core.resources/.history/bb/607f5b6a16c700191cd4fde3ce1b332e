class List{
	private ArrayList<int[]> list = new ArrayList<int[]>();
	
	public void Add(int el) {
		list.add(el);
	}
	
	public void Show() {
		for (int el: list.get(list.size())) {
			System.out.print(el + " ");
		}
			
	}
	
	
	
}
