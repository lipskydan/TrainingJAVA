package Array;

import java.util.*;

public class Array {

	public static void main(String[] args) {
		System.out.println("¬ведите число: ");
		
		Scanner in = new Scanner(System.in);
		int size = in.nextInt();
		int[] a = new int[size];
		
		for (int i=0; i < a.length; i++) a[i] = i;
		
		for(int el: a) {
			if (el%2 != 0) System.out.print(" ");
			System.out.println(el);
		}
	}

}
