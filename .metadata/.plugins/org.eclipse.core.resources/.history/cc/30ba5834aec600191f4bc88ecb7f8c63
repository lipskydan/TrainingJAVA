package Array;

import java.util.*;


public class Array {

	public static void main(String[] args) {
		System.out.println("¬ведите число: ");
		
		Scanner in = new Scanner(System.in);
		int size = in.nextInt();
		int[] a = new int[size];
		
		for (int index=0, el=1; index < a.length; index++, el++) {
			a[index] = el;
		}
		
		for(int el: a) {
			if (el%2 != 0) System.out.print(" ");
			System.out.println(el);
		}
	}

}
