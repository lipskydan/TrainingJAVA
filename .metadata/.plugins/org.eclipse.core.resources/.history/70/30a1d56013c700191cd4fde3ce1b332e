class List{
	private int size = 0;
	private int[] list = new int[size];
	
	
	public void Add (int el) {
		list[size] = el;
		size++;
	}
	
	public void Show() {
		for (int el: list) {
			System.out.print(el + " ");
		}
	}
	
	
}
